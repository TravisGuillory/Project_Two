{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    },
    "extends": [
        "plugin:prettier/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "moment": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "prefer-const": "error",
        "no-var": "error"
    }
}
